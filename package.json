{
  "version": "1.0.0",
  "name": "node-cli-temple",
  "description": "node-cli-temple",
  "keywords": [
    "node",
    "cli",
    "node-cli-temple"
  ],
  "author": "template-user",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bridgewwater/node-cli-temple.git"
  },
  "homepage": "https://github.com/bridgewwater/node-cli-temple#readme",
  "bugs": {
    "url": "https://github.com/bridgewwater/node-cli-temple/issues"
  },
  "bin": {
    "node-cli-temple": "./bin/node-cli-temple.js"
  },
  "scripts": {
    "cli:help": "node bin/node-cli-temple.js -h",
    "clean:rimraf": "npx rimraf node_modules package-lock.json",
    "clean:lockfile": "npx replace-lockfile-registry --registry https://registry.npmjs.org/",
    "format": "npx prettier --write eslint.config.mjs \"{bin,src,tests}/**/*.{js,vue,ts,tsx}\"",
    "lint:eslint": "npx eslint \"bin/**/*.js\" \"src/**/*.js\" \"tests/**/*.js\"",
    "lint:eslintNoWarning": "npx eslint --cache --max-warnings 0 \"{bin,src,tests}/**/*.{js,vue,ts,tsx}\" --fix",
    "test": "npx jest --ci",
    "jest:coverage": "npx jest --ci --coverage",
    "jest:collectCoverage": "npx jest --collectCoverage --silent"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^10.1.6",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "minimatch": "^10.0.1",
    "moment": "^2.30.1",
    "shelljs": "^0.8.5",
    "string-format": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "eslint": "^9.8.0",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "replace-lockfile-registry": "^0.0.2",
    "rimraf": "6.0.1",
    "semver": "^7.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
